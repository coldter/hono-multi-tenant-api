{
  "name": "hono-node-starter",
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "main": ".output/main.js",
  "scripts": {
    "load-env": "dotenv -e .env.local --",
    "dev": "dotenv -e .env.local -- tsx watch --clear-screen=false --import ./src/tracing.ts ./src/main.ts",
    "start:prod": "node --import ./.output/tracing.js .output/main.js",
    "start": "dotenv -e .env.local -- node --import ./.output/tracing.js .output/main.js",
    "build": "tsup",
    "check": "tsc --noEmit",
    "fmt": "npx biome format . --write && npx biome check . --write",
    "db:generate": "npm run load-env drizzle-kit generate --config=drizzle.config.ts",
    "db:push": "npm run load-env drizzle-kit push --config=drizzle.config.ts",
    "db:check": "npm run load-env drizzle-kit check --config=drizzle.config.ts",
    "db:clean": "npm run load-env tsx src/database/dbClean.ts",
    "db:up": "npm run load-env drizzle-kit up --config=drizzle.config.ts",
    "db:drop": "npm run load-env drizzle-kit drop --config=drizzle.config.ts",
    "db:studio": "npm run load-env drizzle-kit studio --port 3333  --config=drizzle.config.ts",
    "db:migrate": "npm run load-env tsx src/database/migrate.ts",
    "ts:check": "tsc --noEmit",
    "new:v1apiPublic": "hypergen new v1api",
    "new:v1api": "hypergen new v1apiWithAuth"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hono/node-server": "^1.13.7",
    "@hono/zod-openapi": "^0.18.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/api-logs": "^0.55.0",
    "@opentelemetry/exporter-logs-otlp-http": "^0.55.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.55.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.55.0",
    "@opentelemetry/instrumentation": "^0.55.0",
    "@opentelemetry/instrumentation-http": "^0.55.0",
    "@opentelemetry/instrumentation-undici": "^0.8.0",
    "@opentelemetry/instrumentation-winston": "^0.42.0",
    "@opentelemetry/resources": "^1.28.0",
    "@opentelemetry/sdk-logs": "^0.55.0",
    "@opentelemetry/sdk-trace-node": "^1.28.0",
    "@opentelemetry/winston-transport": "^0.8.0",
    "@scalar/hono-api-reference": "^0.5.159",
    "chalk": "^5.3.0",
    "change-case": "^5.4.4",
    "cli-highlight": "^2.1.11",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.36.3",
    "firebase-admin": "^13.0.0",
    "hono": "^4.6.10",
    "ioredis": "^5.4.1",
    "itty-time": "^1.0.6",
    "libphonenumber-js": "^1.11.14",
    "lucia": "^3.2.2",
    "oslo": "^1.2.1",
    "pg": "^8.13.1",
    "pg-error-enum": "^0.7.3",
    "rate-limiter-flexible": "^5.0.4",
    "typeid-js": "^1.1.0",
    "ua-parser-js": "2.0.0-beta.3",
    "unstorage": "^1.13.1",
    "winston": "^3.17.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/ejs": "^3.1.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.9.0",
    "@types/pg": "^8.11.10",
    "@types/ua-parser-js": "^0.7.39",
    "dotenv-cli": "^7.4.2",
    "drizzle-kit": "^0.28.1",
    "ejs": "^3.1.10",
    "enquirer": "^2.4.1",
    "firebase": "^11.0.2",
    "fs-extra": "^11.2.0",
    "hypergen": "7.0.0-rc.2",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "type-fest": "^4.27.0",
    "typescript": "^5.6.3"
  }
}
